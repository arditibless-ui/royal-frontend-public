{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "",
    "NEXT_PUBLIC_USE_MOCK_API": "false",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
